.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admin_live_news_index_url
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_live_news_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      LiveNews
    .inner
      = form_for :live_news, :url => admin_live_news_index_url, :html => { :class => :form, :multipart=>true  } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
