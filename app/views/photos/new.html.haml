.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", photos_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_photo_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      Photo
    .inner
      = form_for :photo, :url => photos_path, :html => { :class => :form, :multipart =>true } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
