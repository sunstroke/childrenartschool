.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admin_live_news_index_url
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_live_news_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_admin_live_news_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      LiveNews
    .inner
      = form_for @live_news, :url => admin_live_news_path(@live_news), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
