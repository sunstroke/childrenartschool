.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", photos_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_photo_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_photo_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      Photo
    .inner
      = form_for @photo, :url => photo_path(@photo), :html => { :class => :form,  :multipart =>true } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
